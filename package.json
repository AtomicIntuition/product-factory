{
  "name": "factory",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:all": "npx concurrently \"npm run dev\" \"npm run dev:backend\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
